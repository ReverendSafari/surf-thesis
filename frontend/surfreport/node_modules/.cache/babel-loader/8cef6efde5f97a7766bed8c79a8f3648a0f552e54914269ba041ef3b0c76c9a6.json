ault {\n    data() {\n      return {\n        isFaqOpen: false, // Tracks if the FAQ modal is open\n      };\n    },\n    methods: {\n      toggleFaq() {\n        this.isFaqOpen = !this.isFaqOpen;\n      },\n    },\n  };\n  </script>\n  \n  <style scoped>\n  /* Add additional styles if needed */\n  </style>\n  "],"mappings":"AAuCE,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE,KAAK,CAAE;IACpB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,SAASA,CAAA,EAAG;MACV,IAAI,CAACF,SAAQ,GAAI,CAAC,IAAI,CAACA,SAAS;IAClC;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}