{"ast":null,"code":"import axios from 'axios';\nimport { GMapMap, GMapMarker } from '@fawmi/vue-google-maps'; // Ensure this package is installed\n\nexport default {\n  components: {\n    GMapMap,\n    GMapMarker\n  },\n  props: {\n    beach: String // Beach name passed from the parent component\n  },\n  data() {\n    return {\n      beachName: '',\n      town: '',\n      state: '',\n      beachImageUrl: '',\n      beachLat: null,\n      beachLong: null\n    };\n  },\n  methods: {\n    async fetchBeachData() {\n      try {\n        const response = await axios.get('https://3k7dz8sjwk.execute-api.us-east-2.amazonaws.com/TestStage/beaches');\n        const beachData = response.data.beaches.find(b => b.beach_name === this.beach);\n        if (beachData) {\n          this.beachName = beachData.beach_name;\n          this.town = beachData.town;\n          this.state = beachData.state;\n          this.beachImageUrl = require(`@/assets/${beachData.beach_name}.jpg`);\n          this.beachLat = beachData.lat;\n          this.beachLong = beachData.long;\n        }\n      } catch (error) {\n        console.error('Error fetching beach data:', error);\n      }\n    }\n  },\n  watch: {\n    beach: {\n      immediate: true,\n      handler() {\n        this.fetchBeachData();\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","GMapMap","GMapMarker","components","props","beach","String","data","beachName","town","state","beachImageUrl","beachLat","beachLong","methods","fetchBeachData","response","get","beachData","beaches","find","b","beach_name","require","lat","long","error","console","watch","immediate","handler"],"sources":["/home/safari/thesis/frontend/surfreport/src/components/BeachAndMapDisplay.vue"],"sourcesContent":["<template>\n    <div class=\"p-4 border rounded-lg w-full max-w-md bg-white shadow-md\">\n      <!-- Beach Information -->\n      <h1 class=\"text-3xl font-bold mb-1\">{{ beachName }} Surf Report</h1>\n      <p class=\"text-gray-600 mb-4\">Surf Report for {{ town }}, {{ state }}</p>\n  \n      <!-- Beach Image -->\n      <div class=\"relative mb-4\">\n        <img :src=\"beachImageUrl\" :alt=\"beachName\" class=\"w-full h-auto aspect-square object-cover rounded-lg shadow-md\" />\n      </div>\n  \n      <!-- Map Section -->\n      <h3 class=\"text-xl mb-4\">Beach Location</h3>\n      <GMapMap\n        :center=\"{ lat: beachLat, lng: beachLong }\"\n        :zoom=\"12\"\n        style=\"width: 100%; height: 300px\"\n      >\n        <GMapMarker :position=\"{ lat: beachLat, lng: beachLong }\" />\n      </GMapMap>\n    </div>\n  </template>\n  \n  <script>\n  import axios from 'axios';\n  import { GMapMap, GMapMarker } from '@fawmi/vue-google-maps'; // Ensure this package is installed\n  \n  export default {\n    components: {\n      GMapMap,\n      GMapMarker,\n    },\n    props: {\n      beach: String, // Beach name passed from the parent component\n    },\n    data() {\n      return {\n        beachName: '',\n        town: '',\n        state: '',\n        beachImageUrl: '',\n        beachLat: null,\n        beachLong: null,\n      };\n    },\n    methods: {\n      async fetchBeachData() {\n        try {\n          const response = await axios.get('https://3k7dz8sjwk.execute-api.us-east-2.amazonaws.com/TestStage/beaches');\n          const beachData = response.data.beaches.find(b => b.beach_name === this.beach);\n          if (beachData) {\n            this.beachName = beachData.beach_name;\n            this.town = beachData.town;\n            this.state = beachData.state;\n            this.beachImageUrl = require(`@/assets/${beachData.beach_name}.jpg`);\n            this.beachLat = beachData.lat;\n            this.beachLong = beachData.long;\n          }\n        } catch (error) {\n          console.error('Error fetching beach data:', error);\n        }\n      },\n    },\n    watch: {\n      beach: {\n        immediate: true,\n        handler() {\n          this.fetchBeachData();\n        },\n      },\n    },\n  };\n  </script>\n  \n  <style scoped>\n  @import url('https://fonts.googleapis.com/css2?family=Rubik:wght@400&display=swap');\n  \n  .heading-text {\n    text-align: left;\n    font-family: 'Rubik', sans-serif;\n    color: black;\n  }\n  \n  .font-jakarta {\n    font-family: 'Plus Jakarta Sans', sans-serif;\n  }\n  \n  img {\n    object-position: center;\n  }\n  </style>\n  "],"mappings":"AAwBE,OAAOA,KAAI,MAAO,OAAO;AACzB,SAASC,OAAO,EAAEC,UAAS,QAAS,wBAAwB,EAAE;;AAE9D,eAAe;EACbC,UAAU,EAAE;IACVF,OAAO;IACPC;EACF,CAAC;EACDE,KAAK,EAAE;IACLC,KAAK,EAAEC,MAAM,CAAE;EACjB,CAAC;EACDC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE,EAAE;MACbC,IAAI,EAAE,EAAE;MACRC,KAAK,EAAE,EAAE;MACTC,aAAa,EAAE,EAAE;MACjBC,QAAQ,EAAE,IAAI;MACdC,SAAS,EAAE;IACb,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,cAAcA,CAAA,EAAG;MACrB,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMhB,KAAK,CAACiB,GAAG,CAAC,0EAA0E,CAAC;QAC5G,MAAMC,SAAQ,GAAIF,QAAQ,CAACT,IAAI,CAACY,OAAO,CAACC,IAAI,CAACC,CAAA,IAAKA,CAAC,CAACC,UAAS,KAAM,IAAI,CAACjB,KAAK,CAAC;QAC9E,IAAIa,SAAS,EAAE;UACb,IAAI,CAACV,SAAQ,GAAIU,SAAS,CAACI,UAAU;UACrC,IAAI,CAACb,IAAG,GAAIS,SAAS,CAACT,IAAI;UAC1B,IAAI,CAACC,KAAI,GAAIQ,SAAS,CAACR,KAAK;UAC5B,IAAI,CAACC,aAAY,GAAIY,OAAO,CAAC,YAAYL,SAAS,CAACI,UAAU,MAAM,CAAC;UACpE,IAAI,CAACV,QAAO,GAAIM,SAAS,CAACM,GAAG;UAC7B,IAAI,CAACX,SAAQ,GAAIK,SAAS,CAACO,IAAI;QACjC;MACF,EAAE,OAAOC,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,4BAA4B,EAAEA,KAAK,CAAC;MACpD;IACF;EACF,CAAC;EACDE,KAAK,EAAE;IACLvB,KAAK,EAAE;MACLwB,SAAS,EAAE,IAAI;MACfC,OAAOA,CAAA,EAAG;QACR,IAAI,CAACf,cAAc,CAAC,CAAC;MACvB;IACF;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}